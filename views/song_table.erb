<p><%= @table_desc %></p>
<table>
  <tr>
    <th>Id </th>
    <th>Title </th>
    <th>Artist</th>
    <th>Album</th>
    <th>Genre</th>
    <th>Year</th>
    <th>BPM</th>
    <th>Updated_At</th>
  </tr>
  <% @songs.each do |song| %>
  <tr>
    <td><%= song[:song_id] %></td>
    <td><%= song[:name] %></td>
    <td><%= song[:artist] %></td>
    <td><%= song[:album] %></td>
    <td><%= song[:genre] %></td>
    <td><%= song[:year] %></td>
    <td><%= song[:bpm] %></td>
    <td><%= song[:updated_at].strftime("%m/%d/%Y %H:%M") %></td>
  </tr>
  <% end %>
</table>
